{
  "name": "nextfastapi",
  "version": "2.2.0",
  "description": "Next.js API controller with middleware and error handling support.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./types": {
      "types": "./dist/types/Controller.d.ts"
    },
    "./errors": {
      "import": "./dist/errors/index.js",
      "types": "./dist/errors/index.d.ts"
    },
    "./http": {
      "import": "./dist/http/index.js",
      "types": "./dist/http/index.d.ts"
    },
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "author": "Ezequias Lopes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/silvaezequias/nextfastapi.git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w"
  },
  "keywords": [
    "nextjs",
    "api",
    "controller",
    "middleware",
    "typescript"
  ],
  "dependencies": {
    "snakecase-keys": "^8.0.1"
  },
  "devDependencies": {
    "@types/next": "^9.0.0",
    "typescript": "^5.4.0"
  }
}
